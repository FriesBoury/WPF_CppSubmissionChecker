<UserControl x:Class="CppSubmissionChecker_View.UserControls.TextLog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:CppSubmissionChecker_ViewModel.Viewmodels.TextLog;assembly=CppSubmissionChecker_ViewModel"
             xmlns:local="clr-namespace:CppSubmissionChecker_View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <vm:TextLog_VM/>
    </UserControl.DataContext>
    <TextBox x:Name="console" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" TextWrapping="Wrap" FontFamily="Consolas"  Background="Black" Foreground="White" IsReadOnly="True"></TextBox>
</UserControl>
